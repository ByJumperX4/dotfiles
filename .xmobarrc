Config { bgColor = "#000000"
       , fgColor = "#7fff7f"
       , position = TopW L 95
       , commands = [ Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
		    , Run Date "%H:%M:%S - %d/%m/%Y" "date" 10
                    , Run StdinReader
		    , Run Com "uname" ["-rs"] "" 0
		    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% : %cpu% : %memory% }{<fc=#7fff7f>%date%</fc> : %uname% "
       } 